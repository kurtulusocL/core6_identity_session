@model ProfilePhoto
@{
    Layout = null;
}

@if (Model != null)
{
    <img src="~/img/profile/@Model.Image" style="max-height:90px;max-width:80px;min-height:90px;min-width:80px" class="img-responsive" alt="@Model.Image">
}
else
{
    <img src="~/img/statics/userImage.jpg" style="max-height:90px;max-width:80px;min-height:90px;min-width:80px" class="img-responsive">
}